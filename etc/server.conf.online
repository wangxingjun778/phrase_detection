[server]
#服务名(全称)：命名空间$服务名简称, client端请求server的标识 
service=xinhuanet$Nlp$Phrase
#server服务端口
port=9117
#zookeeper的连接字符串
zk_connect_str=10.122.141.27:2181, 10.122.141.28:2181, 10.122.141.29:2181
#server授权用户字符串 避免其他服务也注册此服务
auth_user=phrase
#server授权用户字符串 避免其他服务也注册此服务名
auth_password=phrase
#是否监控server的状态和请求数等信息， 默认值是False
monitor=True
#server服务的名称
name=xinhuanetNlpPhrase
#server 服务负责人 
owner=jinhuang.yu@baifendian.com
#server 开启的进程数 默认值10个
process_num=3
#server 每个进程中的协程数目，默认值100个
coroutines_num=6
